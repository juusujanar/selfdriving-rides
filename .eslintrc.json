{
    "extends": "airbnb-base",
    "env": {
        "browser": true
    },
    "rules": {
        "import/prefer-default-export": "off",
        "import/no-default-export": "on",
        "no-plusplus": "off",
        "no-console": "off",
        "object-curly-newline": "off",
        "no-param-reassign": "off"
    }
}
